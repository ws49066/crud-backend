// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://admin:admin@localhost:27017/crud?authSource=crud&replicaSet=rs0"
}


model Clientes {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name    String?
  email   String   @unique
}